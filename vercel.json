{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "regions": ["hnd1"],
    "functions": {
      "api/**/*.py": {
        "maxDuration": 30,
        "memory": 1024
      }
    },
    "routes": [
      { "src": "/healthz", "dest": "/api/index.py" },
      { "src": "/(.*)", "dest": "/api/index.py" }
    ],
    "env": { "PYTHONPATH": "." }
  }
  